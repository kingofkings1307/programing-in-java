import java.util.*;
class Main 
{
public static void main(String args[])
{
int i, j; 
Scanner input = new Scanner (System.in);
int x = input.nextInt();
for (i=0; i<x; i++) 
{
for (j=x-i; j>1; j--) 
{
System.out.print(" ");
}
for (j=0; j<=i; j++ )
{
System.out.print(" "); 
} 
System.out.printin();
}
}
}
